{
  "name": "battle-royal",
  "version": "1.1.0",
  "description": "battle royal api",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build . -s --ignore 'node_modules/**'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node --trace-warnings ./build/server.js",
    "lint": "eslint . --fix",
    "dev": "nodemon --exec babel-node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satishjhanwer/battle-royal.git"
  },
  "keywords": [
    "Express",
    "MongoDB",
    "MLab",
    "Mongoose",
    "Node",
    "JWT",
    "Heroku"
  ],
  "author": "Satish Jhanwer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/satishjhanwer/battle-royal/issues"
  },
  "homepage": "https://github.com/satishjhanwer/battle-royal#readme",
  "dependencies": {
    "async": "3.2.4",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.0",
    "csvtojson": "2.0.10",
    "express": "4.18.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "mongoose": "6.4.4"
  },
  "devDependencies": {
    "@babel/cli": "7.18.6",
    "@babel/core": "7.18.6",
    "@babel/eslint-parser": "7.18.2",
    "@babel/preset-env": "7.18.6",
    "babel-register": "6.26.0",
    "eslint": "8.19.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
