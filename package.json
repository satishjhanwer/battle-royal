{
  "name": "battle-royal",
  "version": "1.1.0",
  "description": "battle royal api",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build . -s --ignore 'node_modules/**'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node --trace-warnings ./build/server.js",
    "lint": "eslint . --fix",
    "dev": "nodemon --exec babel-node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satishjhanwer/battle-royal.git"
  },
  "keywords": [
    "Express",
    "MongoDB",
    "MLab",
    "Mongoose",
    "Node",
    "JWT",
    "Heroku"
  ],
  "author": "Satish Jhanwer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/satishjhanwer/battle-royal/issues"
  },
  "homepage": "https://github.com/satishjhanwer/battle-royal#readme",
  "dependencies": {
    "async": "3.2.6",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.3",
    "csvtojson": "2.0.10",
    "express": "4.21.1",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "mongoose": "8.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.25.7",
    "@babel/core": "7.25.8",
    "@babel/eslint-parser": "7.25.8",
    "@babel/preset-env": "7.25.8",
    "eslint": "8.38.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
