{
  "name": "battle-royal",
  "version": "1.1.0",
  "description": "battle royal api",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build . -s --ignore 'node_modules/**' --presets=es2015-node5",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/server.js",
    "lint": "eslint . --fix",
    "dev": "nodemon --exec babel-node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satishjhanwer/battle-royal.git"
  },
  "keywords": [
    "Express",
    "MongoDB",
    "MLab",
    "Mongoose",
    "Node",
    "JWT",
    "Heroku"
  ],
  "author": "Satish Jhanwer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/satishjhanwer/battle-royal/issues"
  },
  "homepage": "https://github.com/satishjhanwer/battle-royal#readme",
  "dependencies": {
    "async": "3.2.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "csvtojson": "2.0.10",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.19",
    "mongoose": "5.9.7",
    "babel-cli": "6.26.0",
    "babel-preset-es2015-node5": "1.2.0",
    "babel-preset-stage-3": "6.24.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "eslint": "6.8.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
