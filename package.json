{
  "name": "battle-royal",
  "version": "1.0.0",
  "description": "battle royal api",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build . -s --ignore 'node_modules/**' --presets=es2015-node5",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/server.js",
    "dev": "nodemon --exec babel-node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satishjhanwer/battle-royal.git"
  },
  "keywords": [
    "Express",
    "MongoDB",
    "MLab",
    "Mongoose",
    "Node",
    "JWT",
    "Heroku"
  ],
  "author": "Satish Jhanwer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/satishjhanwer/battle-royal/issues"
  },
  "homepage": "https://github.com/satishjhanwer/battle-royal#readme",
  "dependencies": {
    "async": "^2.6.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "csvtojson": "^2.0.8",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-stage-3": "^6.24.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^5.6.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
